{
  "name": "gotech",
  "version": "1.0.0",
  "description": "Website for gotech.io",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./site",
    "build": "npm run sass-prebuild && cuttlebelle",
    "sass-prebuild": "sass ./assets/sass/index.scss ./assets/css/index.css",
    "watch-sass": "sass -w ./assets/sass/index.scss ./assets/css/index.css",
    "watch": "npm run watch-sass & cuttlebelle -w",
    "watch-win": "npm run sass-prebuild && cuttlebelle -w"
  },
  "cuttlebelle": {
    "site": {
      "markdownRenderer": "markdown.js"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^10.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "sass": "^1.45.0"
  }
}
